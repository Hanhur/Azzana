\n    background-image: url(../../img/bestsellers/heading_right.png);\n    background-repeat: no-repeat;\n    background-position: right;\n    top: 8px;\n    right: -90px;\n    left: 0;\n    bottom: 0;\n}\n.bestsellers-block\n{\n    display: grid;\n    grid-template-columns: repeat(4, 1fr);\n    gap: 37px;\n    margin-bottom: 64px;\n}\n.bestsellers-item\n{\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    box-shadow: 0px 0px 67px 5px #00000014;\n    position: relative;\n}\n.bestsellers-ellipse\n{\n    content: '';\n    position: absolute;\n    top: 0;\n    left: 0;\n    padding: 9px;\n}\n.bestsellers-img\n{\n    width: 100%;\n    height: 280px;\n    margin-bottom: 10px;\n}\n.bestsellers-title\n{\n    font-size: 14px;\n    font-weight: 500;\n    letter-spacing: 1.2px;\n    color: var(--Black-1);\n    margin-bottom: 13px;\n}\n.bestsellers_box\n{\n    display: flex;\n    flex-direction: row;\n    justify-content: center;\n    align-items: center;\n    gap: 0 15px;\n    margin-bottom: 13px;\n}\n.bestsellers-span\n{\n    font-size: 14px;\n    font-weight: 600;\n    color: var(--Gray-4);\n}\n.bestsellers_span\n{\n    font-size: 14px;\n    color: var(--Gray-4);\n    text-decoration-line: line-through;\n}\n.bestsellers-box_img\n{\n    display: flex;\n    flex-direction: row;\n    justify-content: center;\n    align-items: center;\n    gap: 0 5px;\n    margin-bottom: 12px;\n}\n.bestsellers-btn\n{\n    font-family: \"Georgian\", serif;\n    font-size: 18px;\n    font-weight: 400;\n    color: var(--Light-Pink);\n    border: 1.7px solid var(--Light-Pink);\n    padding: 13px 43px;\n    display: block;\n    margin: 0 auto;\n}"},"lineCount":null}},"error":null,"hash":"58d5859ca7cc75f3957c4139152864dd","cacheData":{"env":{}}}